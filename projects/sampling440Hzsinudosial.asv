f0 = 440; %440 Hz signal frequency
w0 = 2*pi*f0; %signal frequency in rad/s
Ts = 0.0001; %time interval between samples
n=0:0.01:3; % samples
x1=sin(w0*n); % signal data set
t=0:0.0001:3;

plot(n,x1);
xlim([0 3]);
ylim([-1.5 1.5]);
grid on;
xlabel('sec');
ylabel('amplitude');
title('Sampled Sinosoidal Signal');
sound(x1);

